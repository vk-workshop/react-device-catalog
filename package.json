{
  "homepage": "https://vk-workshop.github.io/react-device-catalog",
  "name": "react-typescript-starter-pack",
  "version": "0.1.0",
  "type": "module",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.5.1",
    "bulma": "^0.9.4",
    "classnames": "^2.3.2",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "openai": "^4.86.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.0",
    "react-scripts": "5.0.1",
    "swiper": "^11.1.9",
    "typescript": "^4.6.3",
    "typescript-eslint-parser": "^22.0.0",
    "web-vitals": "^1.1.1"
  },
  "scripts": {
    "format": "prettier --write './src/**/*.{ts,tsx}'",
    "lint": "npm run format && eslint ./src --fix",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "server": "nodemon server/server.js",
    "dev": "concurrently \"npm start\" \"npm run server\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@mate-academy/eslint-config-react-typescript": "latest",
    "@mate-academy/students-ts-config": "latest",
    "@types/node": "^16.18.80",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "autoprefixer": "^10.4.20",
    "eslint": "^7.32.0",
    "gh-pages": "^6.3.0",
    "globals": "^16.0.0",
    "nodemon": "^3.1.9",
    "postcss": "^8.5.3",
    "prettier": "^3.2.5",
    "sass": "^1.72.0",
    "tailwindcss": "^4.0.9"
  }
}
